{% if admin.isGranted('EDIT', object) and admin.hasRoute('export_yml') %}
    <a href="{{ admin.generateObjectUrl('export_yml', object) }}" class="btn btn-sm btn-default edit_link" title="{{ 'Export yml'|trans({}, 'SonataAdminBundle') }}">
        <i class="fa fa-floppy-o"></i>
        {{ 'Export Yml'|trans({}, 'SonataAdminBundle') }}
    </a>
{% endif %}
