{
    "name": "eight/page-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Eight PageBundle",
    "keywords": ["page", "cms"],
    "homepage": "https://github.com/matteocaberlotto",
    "license": "MIT",
    "authors": [
        {
            "name": "Matteo Caberlotto",
            "homepage": "https://github.com/matteocaberlotto/EightPageBundle/graphs/contributors"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.9",
        "symfony/monolog-bundle": ">=2.1,<3",
        "doctrine/doctrine-bundle": "~1.4",
        "sonata-project/doctrine-extensions": "1.*",
        "raindrop/routing-bundle": "master"
    },
    "require-dev": {
        "symfony/class-loader": "~2.2",
        "symfony/yaml": "~2.2",
        "symfony/form": "~2.2",
        "symfony/finder": "~2.2"
    },
    "autoload": {
        "psr-4": { "Eight\\PageBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
